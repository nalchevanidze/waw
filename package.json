    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.4",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "lerna": "^7.1.1"
  }
}
