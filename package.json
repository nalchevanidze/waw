{
  "name": "wdaw",
  "license": "MIT",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev": "npm run dev -w=@wdaw/docs",
    "start": "concurrently --kill-others 'tsc -b -w' 'webpack serve'",
    "test": "jest",
    "clean": "rm -rf ./packages/*/tsconfig.tsbuildinfo ./packages/*/dist ./packages/*/node_modules",
    "build": "tsc -b",
    "publish": "npm run build && lerna publish --no-private",
    "lint": "eslint .",
    "format": "prettier . --write",
    "check:spelling": "npx cspell --gitignore --no-progress --color **/*.{ts,tsx,md}",
    "check:formating": "prettier . --check"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "concurrently": "^8.0.1",
    "cspell": "^6.31.1",
    "eslint": "^8.44.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "lerna": "^7.1.1"
  }
}
